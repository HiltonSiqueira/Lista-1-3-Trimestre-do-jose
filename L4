let idade = parseInt(prompt("Digite a sua idade:"));

if (idade >= 5 && idade <= 7) {
    console.log("bebe");
} else if (idade >= 8 && idade <= 10) {
    console.log("criança ");
} else if (idade >= 11 && idade <= 13) {
    console.log("pre adolecente");
} else if (idade >= 14 && idade <= 17) {
    console.log("adolecente");
} else if (idade >= 18) {
    console.log("Adulto");
} else {
    console.log("Idade inválida");
}
