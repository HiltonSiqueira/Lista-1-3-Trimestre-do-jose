let saldoMedio = parseFloat(prompt("Digite o saldo médio:"));
let credito;

if (saldoMedio <= 200) {
    credito = 0;
}

if (saldoMedio > 200 && saldoMedio <= 400) {
    credito = saldoMedio * 0.20;
}

if (saldoMedio > 400 && saldoMedio <= 600) {
    credito = saldoMedio * 0.30;
}

if (saldoMedio > 600) {
    credito = saldoMedio * 0.40;
}

alert("Saldo médio: R$ " + saldoMedio.toFixed(2) + "\nCrédito: R$ " + credito.toFixed(2));
